<!doctype html>
<html AMP lang="en">
<head>
    <meta charset="utf-8">
    <title>{{ pageData.title }}</title>
    <meta name="author" content="{{ pageData.author }}" />
    <meta name="description" content="{{ pageData.description }}" />
    <meta name="keywords" content="{{ pageData.keywords }}" />
    <link rel="canonical" href="{{ pageData.canonical }}" />
    <meta name="viewport" content="width=device-width,minimum-scale=1,maximum-scale=1,initial-scale=1">

    <!--*
        *   FavIcons
        **-->
    {% include "templates/parts/_favicons.nunjucks" %}

    <!--*
        *   Fonts
        **-->
    {% include "templates/parts/_fonts.nunjucks" %}

    <!--*
        *   JavaScripts to Include
        **-->
    {%- for script in pageData.scripts -%}
        {% set attrz = "" -%}
        {% if script.tags.attr and script.tags.val -%}
            {% set attrz = script.tags.attr + "=" + script.tags.val -%}
        {%- endif %}
    <script async {{ attrz }} src="{{ script.src }}"></script>
    {%- endfor %}

    <!--*
        *   Structured Data
        **-->
    <script type="application/ld+json">
        {{ pageData.structuredData | safe }}
    </script>

    <!--*
        *   Required CSS Code (AMP Boilerplate)
        **-->
    <style amp-boilerplate>body{-webkit-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-moz-animation:-amp-start 8s steps(1,end) 0s 1 normal both;-ms-animation:-amp-start 8s steps(1,end) 0s 1 normal both;animation:-amp-start 8s steps(1,end) 0s 1 normal both}@-webkit-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-moz-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-ms-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@-o-keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}@keyframes -amp-start{from{visibility:hidden}to{visibility:visible}}</style><noscript><style amp-boilerplate>body{-webkit-animation:none;-moz-animation:none;-ms-animation:none;animation:none}</style></noscript>

    <!--*
        *   Custom CSS Code
        **-->
    <style amp-custom>
        /* GLOBAL STYLES */
        {%- include "templates/css/_global_styles.css" %}
        /* /GLOBAL STYLES */

        /* INDEX PAGE STYLES */

        {% for cssFile in pageData.css -%}

            {% include "templates/css/_" + cssFile + ".css" %}

        {% endfor %}

        /* /INDEX PAGE STYLES */
    </style>
</head>
<body dir="ltr">
    {% include "templates/parts/_header.nunjucks" %}

    {% include pageData.contentFile %}

    {% include "templates/parts/_sidebar.nunjucks" %}
</body>
</html>